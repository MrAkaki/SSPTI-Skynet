# Discord
DISCORD_TOKEN=
CLIENT_ID=
# Optional: set to register slash commands instantly for a single guild
GUILD_ID=

# Comma-separated channel IDs to allow replies in.
# If empty/unset, the bot will respond in any channel it can see.
ALLOWED_CHANNEL_IDS=

# llama.cpp server
# Expected to expose an OpenAI-compatible endpoint at: ${LLAMA_BASE_URL}/v1/chat/completions
LLAMA_BASE_URL=http://127.0.0.1:8080
LLAMA_API_KEY=
LLAMA_MODEL=
LLAMA_TEMPERATURE=0.2
LLAMA_MAX_TOKENS=512

# Knowledge base
KNOWLEDGE_DIR=knowledge
KNOWLEDGE_TOP_K=5

# Logging
# debug | info | warn | error | silent
LOG_LEVEL=info

# Built-in tools
# Pricer (Janice) - required to use the tool.
JANICE_API_KEY=
